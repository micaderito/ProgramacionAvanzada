<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-mapping>
	<class name="com.jwt.hibernate.Persona" table="PERSONA">
		<id column="Dni" name="dni" type="int" />
		<field column="Apellido" name="apellido" type="string" />
		<field column="Nombre" name="nombre" type="string" />
		<field column="Sexo" name="sexo" type="char" />
	</class>
</hibernate-mapping>